<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.ant.freeform</type>
    <configuration>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/1">
            <!-- No utilice el personalizador de propiedades de proyectos cuando edite manualmente este archivo. -->
            <name>Servicio de Pago con VISA</name>
            <properties>
                <property name="ant.script">../build.xml</property>
            </properties>
            <folders>
                <source-folder>
                    <label>client</label>
                    <type>java</type>
                    <location>client</location>
                </source-folder>
                <source-folder>
                    <label>server</label>
                    <type>java</type>
                    <location>server</location>
                </source-folder>
            </folders>
            <ide-actions/>
            <view>
                <items>
                    <source-folder style="packages">
                        <label>client</label>
                        <location>client</location>
                    </source-folder>
                    <source-folder style="packages">
                        <label>server</label>
                        <location>server</location>
                    </source-folder>
                    <source-file>
                        <location>${ant.script}</location>
                    </source-file>
                </items>
                <context-menu/>
            </view>
        </general-data>
        <java-data xmlns="http://www.netbeans.org/ns/freeform-project-java/1">
            <compilation-unit>
                <package-root>client</package-root>
                <source-level>1.5</source-level>
            </compilation-unit>
            <compilation-unit>
                <package-root>server</package-root>
                <source-level>1.5</source-level>
            </compilation-unit>
        </java-data>
    </configuration>
</project>
